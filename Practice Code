CREATE TABLE EMPLOYEES(
EMPLOYEE_ID INT PRIMARY KEY,
EMPLOYEE_NAME VARCHAR(50),
DEPARTMENT VARCHAR(50),
SALARY DECIMAL(10,2),
EMAIL VARCHAR(100)
);


INSERT INTO EMPLOYEES(EMPLOYEE_ID, EMPLOYEE_NAME, DEPARTMENT, SALARY,EMAIL) 
